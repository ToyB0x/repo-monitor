{
  "name": "@ts-bench/web-monitor",
  "private": true,
  "type": "module",
  "scripts": {
    "// TODO": "NPM経由で利用するユーザのためにDBパスを.env以外のコマンドオプション等で渡せるよう改修",
    "// NOTE: vite付属の.env自動読み込み機能だけではPrsimaの初期化時に失敗するためdotenvx経由で環境変数を渡す": "",
    "dev": "dotenvx run -f .env -- react-router dev",
    "// NOTE: prerender コマンド実行時に DB を読み込むため事前にDBマイグレーションが必要": "",
    "build:prerender": "dotenvx run -f .env -- react-router build",
    "start": "http-server -c-1 ./build/client",
    "typegen": "react-router typegen",
    "typecheck": "npm run typegen && tsc --noEmit"
  },
  "dependencies": {
    "@react-router/node": "7.6.2",
    "@react-router/serve": "7.6.2",
    "@ts-bench/db": "workspace:*",
    "isbot": "5.1.28",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.44.2",
    "@react-router/dev": "7.6.2",
    "@tailwindcss/vite": "4.1.10",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.15.32",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "http-server": "14.1.1",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4"
  }
}
