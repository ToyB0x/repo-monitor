{
  "name": "@ts-bench/cli",
  "version": "0.0.10",
  "type": "module",
  "bin": "./src/index.ts",
  "//NOTE: tsx経由で実行するようにindex.tsを設定してあるため main フィールドは不要": "",
  "//main": "dist/index.js",
  "scripts": {
    "NOTE: dbパッケージをビルドしていないことと関係してviteかtsx経由で実行しないとimportエラー等が発生するため dev/build コマンドはコメントアウト": "",
    "//dev": "tsup --watch",
    "//build": "tsup",
    "//analyze": "node . analyze",
    "test": "vitest",
    "execute": "tsx ./src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ts-bench/db": "workspace:*",
    "@supercharge/promise-pool": "3.2.0",
    "@typescript/analyze-trace": "0.10.1",
    "commander": "14.0.0",
    "simple-git": "3.28.0",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "24.0.3",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "author": "ToyB0x",
  "license": "MIT",
  "homepage": "https://github.com/ToyB0x/ts-bench",
  "bugs": {
    "url": "https://github.com/ToyB0x/ts-bench/issues"
  },
  "files": [
    "src/",
    "tsconfig.json"
  ]
}
