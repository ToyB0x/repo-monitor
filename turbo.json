{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
