{
  "name": "@ts-bench/db",
  "version": "0.0.3",
  "type": "module",
  "main": "./dist/client.js",
  "exports": {
    ".": {
      "types": "./src/client.ts",
      "default": "./dist/client.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "generate": "prisma generate",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "// prisma コマンドは直接.envを読めるが、.env中の変数展開ができないことと、他コマンドと統一するためにdotenvxを利用": "",
    "format": "prisma format",
    "db:migrate:deploy": "dotenvx run -f .env -- prisma migrate deploy",
    "db:migrate:dev": "dotenvx run -f .env -- prisma migrate dev",
    "db:push": "dotenvx run -f .env -- prisma db push",
    "studio": "dotenvx run -f .env -- prisma studio",
    "typecheck": "tsc --noEmit",
    "// Seed用のtsxコマンドはprisma.schema経由でenvを読めないのでdotenvxでenvを読み込む": "",
    "db:seed": "dotenvx run -f .env -- tsx src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.9.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.44.2",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "24.0.3",
    "prisma": "6.9.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "author": "ToyB0x",
  "license": "MIT",
  "homepage": "https://github.com/ToyB0x/ts-bench",
  "bugs": {
    "url": "https://github.com/ToyB0x/ts-bench/issues"
  },
  "files": [
    "dist/"
  ]
}
